<p id="notice"><%= notice %></p>

<h1>Listing Lideres</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Telefone</th>
      <th>Residencial</th>
      <th>Celular</th>
      <th>Endereco</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lideres.each do |lider| %>
      <tr>
        <td><%= lider.nome %></td>
        <td><%= lider.cpf %></td>
         <td><%= lider.telefone %></td>
        <td><%= lider.residencial %></td>
        <td><%= lider.celular %></td>
        <td><%= lider.Endereco %></td>
        <td><%= lider.Bairro %></td>
        <td><%= lider.Cidade %></td>
        <td><%= lider.Estado %></td>
        <td><%= link_to 'Show', lider %></td>
        <td><%= link_to 'Edit', edit_lider_path(lider) %></td>
        <td><%= link_to 'Destroy', lider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lider', new_lider_path %>
